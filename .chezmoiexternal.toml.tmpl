{{ if lt (output "tmux" "-V") "tmux 2.4" }}
[".tmux.conf"]
    type = "file"
    url = "https://raw.githubusercontent.com/alekseik1/chezmoi-setup/master/.tmux.conf.old"
{{ else }}
[""]
    type = "archive"
    url = "https://github.com/gpakosz/.tmux/archive/fd1bbb56148101f4b286ddafd98f2ac2dcd69cd8.zip"
    extract = true
    stripComponents = 1
    refreshPeriod = "168h"
    include = ["*/.tmux.conf"]
{{ end }}
